<!DOCTYPE resources SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name=" Login Test" verbose="1" parallel="tests">

    <test name="Login Test on chrome" parallel="methods">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="DNSG.com.testcases.LoginTest"/>
        </classes>
    </test>

    <test name="Login Test on Firefox" parallel="methods">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="DNSG.com.testcases.LoginTest"/>
        </classes>
    </test>

    <test name="Login Test on edge" parallel="methods">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="DNSG.com.testcases.LoginTest"/>
        </classes>
    </test>

</suite>